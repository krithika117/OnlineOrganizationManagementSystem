@using Microsoft.AspNetCore.Identity
@inject UserManager<IdentityUser> UserManager
<a asp-action="Create" class="btn btn-primary">+ Create New</a>

<h2 class="py-2">Users</h2>
<table class="table">
    <thead>
        <tr>
            <th>User ID</th>
            <th>Email</th>
         
        </tr>
    </thead>
    <tbody>
        @foreach (var user in UserManager.Users)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Email</td>
         
            </tr>
        }
    </tbody>
</table>
